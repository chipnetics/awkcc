i pkginfo
i copyright

f none /usr/lbin/awkcc 555 bin bin

f none /usr/man/u_man/man1/awkcc.1 444 bin bin

d none /opt 0755 bin bin
d none /opt/awkcc 0755 bin bin
d none /opt/awkcc/bin 0755 bin bin

f none /opt/awkcc/bin/awkcc 555 bin bin
f none /opt/awkcc/bin/awkcc.sh 555 bin bin
f none /opt/awkcc/bin/libAWK.a 444 bin bin

d none /opt/awkcc/include 0755 bin bin

f none /opt/awkcc/include/awk.h 444 bin bin
f none /opt/awkcc/include/dollars.h 444 bin bin
f none /opt/awkcc/include/ear.h 444 bin bin
f none /opt/awkcc/include/hash.h 444 bin bin
f none /opt/awkcc/include/header.h 444 bin bin
f none /opt/awkcc/include/specassign.h 444 bin bin
f none /opt/awkcc/include/y.tab.h 444 bin bin
